  <appSettings>
    <!--EdgeAX CommerceLink CRT Enabling Configuration Start-->
    <!--Make sure just enable one CRT at a time-->
    <add key="LocalBasePath" value="\EdgeAXCommerceLink" />
    <add key="IsLoadTemplateFromDatabase" value="true" />
    <add key="ErpAdapter" value="VSI.EDGEAXConnector.AXAdapter" />
    <!--EcomAdapter can have three possible values at this time. VSI.CommerceLink.DemandwareAdapter or VSI.CommerceLink.MagentoAdapter or VSI.CommerceLink.ThirdPartyAdapter -->
    <add key="EcomAdapter" value="VSI.CommerceLink.MagentoAdapter" />
    <add key="EnableCRTAX2012R3" value="false" />
    <add key="EnableCRTAX7" value="false" />
    <add key="EnableCRTAX7Local" value="false" />
    <add key="EnableCRTD365" value="true" />
    <!--EdgeAX CommerceLink CRT Enabling Configuration End-->
    <add key="GDPRMethodNames" value="" />
    <add key="UseReadChangedProducts" value="true" />
    <add key="UseGetActivePrices" value="true" />
    <add key="ThreadCountOfJobs" value="30" />
    <add key="JobsDelayInSeconds" value="30" />
    <add key="MissFireThresholdInSeconds" value="120" />
    <add key="STORESKEY" value="" />
    <!--Use only when STORESKEY is empty. This discards the menthioned storekeys when adding Jobs-->
    <add key="IgnoreStoreKeys" value="" />
    <add key="isCacheDisable" value="true" />
  </appSettings>
  <entityFramework>
    <defaultConnectionFactory type="System.Data.Entity.Infrastructure.LocalDbConnectionFactory, EntityFramework">
      <parameters>
        <parameter value="v11.0" />
