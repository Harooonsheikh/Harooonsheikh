<UserControl x:Class="VSI.EDGEAXConnector.UI.UserControlls.Services"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:userControl="clr-namespace:VSI.EDGEAXConnector.UI.UserControlls"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" Background="White">
    <Grid>
        <Border Grid.Row="1" Margin="0,1,0,0" Background="#ffffff">
            <TabControl TabStripPlacement="Top" BorderThickness="1">
                <TabItem Header="CommerceLink Services" Style="{DynamicResource TabItemStyle}" >
                    <Border Margin="0,5,0,0">
                        <userControl:CommerceLinkServices/>
                    </Border>
                </TabItem>
                <TabItem Header="CommerceLink Files" Style="{DynamicResource TabItemStyle}">
                    <Border Margin="0,5,0,0">
                        <userControl:CommerceLinkFiles/>
                    </Border>
                </TabItem>
            </TabControl>
        </Border>
    </Grid>
</UserControl>
